#include<stdio.h>
int main()
{
    int a,b,c;
    scanf("%d %d %d",&a,&b,&c);
    if(a<=b&&a<=c)
    {
        printf("first number is smallest");
        if(a==b)
        {
            printf(" And second number also smallest");
            if(a==c)
            {
                printf(" And third number also smallest");
            }
        }
        else if(a==c)
        {
            printf(" And third number also smallest");
        }
    }
    else if(b<=a&&b<=c)
    {
        printf("the second number is smallest");
        if(b==c)
        {
            printf(" And third number also smallest");
        }
    }
    else
    {
        printf("third number is smallest");
    }
}
