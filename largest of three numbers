#include<stdio.h>
int main()
{
    int a,b,c;
    scanf("%d %d %d",&a,&b,&c);
    if(a>=b&&a>=c)
    {
        printf("first number is largest");
        if(a==b)
        {
            printf(" And second number also largest");
            if(a==c)
            {
                printf(" And third number also largest");
            }
        }
        else if(a==c)
        {
            printf(" And third number also largest");
        }
    }
    else if(b>=a&&b>=c)
    {
        printf("the second number is largest");
        if(b==c)
        {
            printf(" And third number also largest");
        }
    }
    else
    {
        printf("third number is largest");
    }
}
